{
  "name": "odot",
  "private": true,
  "version": "1.0.18",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "docker:build": "docker build -t ghcr.io/tastaturkneter/odot:$(node -p \"require('./package.json').version\") -t ghcr.io/tastaturkneter/odot:latest .",
    "docker:push": "docker push ghcr.io/tastaturkneter/odot:$(node -p \"require('./package.json').version\") && docker push ghcr.io/tastaturkneter/odot:latest",
    "release:patch": "npm version patch && npm run docker:build && npm run docker:push",
    "release:minor": "npm version minor && npm run docker:build && npm run docker:push",
    "release:major": "npm version major && npm run docker:build && npm run docker:push"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@evolu/common": "^7.4.1",
    "@evolu/react": "^10.4.0",
    "@evolu/react-web": "^2.4.0",
    "@evolu/web": "^2.4.0",
    "@tailwindcss/vite": "^4.1.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "html5-qrcode": "^2.3.8",
    "lucide-react": "^0.563.0",
    "qrcode.react": "^4.2.0",
    "radix-ui": "^1.4.3",
    "react": "^19.2.0",
    "react-day-picker": "^9.13.2",
    "react-dom": "^19.2.0",
    "rrule": "^2.8.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vite-pwa/assets-generator": "^1.0.2",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.0.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.56.0",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0"
  },
  "overrides": {
    "minimatch": "^10.2.1"
  }
